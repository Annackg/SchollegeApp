import SwiftUI
enum Tabs: Int {
    case home = 0
    case search = 1
    case profile = 2
    case competitions = 3
}
struct customTabBar: View {
    
    @Binding var selectedTab: Tabs
    var width = UIScreen.main.bounds.width
    var height = UIScreen.main.bounds.height
    
    var body: some View {
